		 			  motor3(power,0);
		 			  motor4(0,0);
		 				HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,1);
		 			  digtemp=225;
		 		  }
		 		  else if(digree>248 && digree<=293){
		 			  if(digtemp!=270){
		 				  motor1(0,0);
		 				  motor2(0,0);
		 				  motor3(0,0);
		 				  motor4(0,0);
		 					HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,0);
		 				xprintf("bleak now\r\n");
		 				  HAL_Delay(bleak_time);
		 			  }
		 			  motor1(power,0);
		 			  motor2(0,power);
		 			  motor3(power,0);
		 			  motor4(0,power);
		 				HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,1);
		 			  digtemp=270;
		 		  }
		 		  else if(digree>293 && digree<=338){
		 			  if(digtemp!=315){
		 				  motor1(0,0);
		 				  motor2(0,0);
		 				  motor3(0,0);
		 				  motor4(0,0);
		 					HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,0);
		 				xprintf("bleak now\r\n");
		 				  HAL_Delay(bleak_time);
		 			  }
		 			  motor1(0,0);
		 			  motor2(0,power);
		 			  motor3(0,0);
		 			  motor4(0,power);
		 				HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,1);
		 			  digtemp=315;
		 		  	  }
		 	  }
		 	  else{
		 		  oled_puts("Running       ST");
		 		  motor1(0,0);
		 		  motor2(0,0);
		 		  motor3(0,0);
		 		  motor4(0,0);
		 			HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,0);
		 		  //digtemp=1000;
		 	  }
		  break;

		  case 250:

		  	   else if(digree>=0 && digree<=360){
		 		  if(digree<=23 || digree>=338){
		 			if(digtemp!=0){
		 					motor1(0,0);
		 					motor2(0,0);
		 					motor3(0,0);
		 					motor4(0,0);
		 					HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,0);
		 					xprintf("bleak now\r\n");
		 					HAL_Delay(bleak_time);
		 			}
		 			motor1(0,power);
		 			motor2(0,power);
		 			motor3(0,power);
		 			motor4(0,power);
		 			HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,1);
		 			  digtemp=0;
		 		  }
		 		  else if(digree>23 && digree<=68){
		 			  if(digtemp!=45){
		 				  motor1(0,0);
		 				  motor2(0,0);
		 				  motor3(0,0);
