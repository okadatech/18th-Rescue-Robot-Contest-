		 			  motor3(0,0);
		 			  motor4(power,0);
		 				HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,1);
		 			  digtemp=135;
		 		  }
		 		  else if(digree>158 && digree<=203){
		 			  if(digtemp!=180){
		 				  motor1(0,0);
		 				  motor2(0,0);
		 				  motor3(0,0);
