		 			  motor3(power,0);
		 			  motor4(0,0);
		 				HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,1);
		 			  digtemp=225;
		 		  }
		 		  else if(digree>248 && digree<=293){
		 			  if(digtemp!=270){
		 				  motor1(0,0);
		 				  motor2(0,0);
		 				  motor3(0,0);
		 				  motor4(0,0);
		 					HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,0);
		 				xprintf("bleak now\r\n");
		 				  HAL_Delay(bleak_time);
		 			  }
		 			  motor1(power,0);
		 			  motor2(0,power);
		 			  motor3(power,0);
		 			  motor4(0,power);
		 				HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,1);
		 			  digtemp=270;
		 		  }
		 		  else if(digree>293 && digree<=338){
		 			  if(digtemp!=315){
		 				  motor1(0,0);
		 				  motor2(0,0);
		 				  motor3(0,0);
		 				  motor4(0,0);
		 					HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,0);
		 				xprintf("bleak now\r\n");
		 				  HAL_Delay(bleak_time);
		 			  }
		 			  motor1(0,0);
		 			  motor2(0,power);
		 			  motor3(0,0);
		 			  motor4(0,power);
		 				HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,1);
		 			  digtemp=315;
		 		  	  }
		 	  }
			  else{
				  motor1(0,0);
				  motor2(0,0);
				  motor3(0,0);
				  motor4(0,0);
					HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,0);
				 // digtemp=1000;
			  }

		  break;

		  case 150:
				//LS
				if(digtemp!=2000){
					motor1(0,0);
					motor2(0,0);
					motor3(0,0);
					motor4(0,0);
					HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,0);
					HAL_Delay(bleak_time);
				}

				motor1(0,power);
				motor2(0,power);
				motor3(power,0);
				motor4(power,0);
				HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,1);
				digtemp=2000;
				HOUI_def=JY901_def_set();
		  break;

		  case 200:
				//RS
				if(digtemp!=3000){
					motor1(0,0);
					motor2(0,0);
					motor3(0,0);
					motor4(0,0);
					HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,0);
