					HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,0);
					//digtemp=1000;
				}
		  break;

		  case 100:
				HOUI=JY901_yaw_get(HOUI_def);
		  	   if(HOUI>= 5 && HOUI<=355){
