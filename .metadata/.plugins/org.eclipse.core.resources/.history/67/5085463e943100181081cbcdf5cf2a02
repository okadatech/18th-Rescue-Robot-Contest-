		 			  motor3(0,power);
		 			  motor4(power,0);
		 				HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,1);
		 			  digtemp=90;
		 		  }
		 		  else if(digree>113 && digree<=158){
