		 			  motor3(power,0);
		 			  motor4(power,0);
		 				HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,1);
		 			  digtemp=180;
		 		  }
		 		  else if(digree>203 && digree<=248){
		 			  if(digtemp!=225){
		 				  motor1(0,0);
		 				  motor2(0,0);
		 				  motor3(0,0);
		 				  motor4(0,0);
		 					HAL_GPIO_WritePin(GPIOE,GPIO_PIN_10,0);
		 				xprintf("bleak now\r\n");
		 				  HAL_Delay(bleak_time);
